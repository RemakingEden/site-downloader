{
  "name": "firefox-download-js",
  "version": "1.0.0",
  "description": "A basic Firefox extension",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "TEST_TYPE=UI vitest run",
    "test:unit": "TEST_TYPE=Unit vitest run",
    "test:integration": "TEST_TYPE=Integration vitest run",
    "test:all": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "web-ext build",
    "start": "web-ext run",
    "web-ext-lint": "web-ext lint",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix"
  },
  "keywords": [
    "firefox",
    "extension"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/runtime": "^7.26.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/ui": "^3.0.8",
    "eslint": "^8.57.0",
    "jsdom": "^26.0.0",
    "vitest": "^3.0.8",
    "web-ext": "^8.4.0"
  }
}
